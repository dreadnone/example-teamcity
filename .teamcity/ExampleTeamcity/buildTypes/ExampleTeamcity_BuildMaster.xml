  <name>build_master</name>
  <description>Build for master branch with deployment to Nexus</description>
  <settings>
    <options>
      <option name="artifactRules" value="target/*.jar =&gt; artifacts" />
    </options>
    <parameters />
    <build-runners>
      <runner id="Maven2" name="Deploy to Nexus" type="Maven2">
        <parameters>
          <param name="goals" value="clean deploy" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="runnerArgs" value="-Dmaven.test.failure.ignore=true" />
          <param name="teamcity.step.mode" value="default" />
          <param name="userSettingsSelection" value="settings.xml" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ExampleTeamcity_HttpsGithubComDreadnoneExampleTeamcityRefsHeadsMaster1" />
